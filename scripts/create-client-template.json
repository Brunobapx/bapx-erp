{
  "easypanel_template": {
    "name": "ERP System Template",
    "description": "Template para criação automática de novas instalações ERP",
    "applications": [
      {
        "name": "erp-cliente-{CLIENT_NUMBER}",
        "type": "application",
        "source": {
          "type": "github",
          "repository": "seu-usuario/seu-repositorio",
          "branch": "main"
        },
        "build": {
          "command": "bun install && bun run build",
          "dockerfile": "./Dockerfile"
        },
        "deploy": {
          "command": "nginx -g \"daemon off;\"",
          "port": 80,
          "healthcheck": {
            "path": "/",
            "timeout": 30
          }
        },
        "environment": {
          "VITE_SUPABASE_URL": "{SUPABASE_URL}",
          "VITE_SUPABASE_ANON_KEY": "{SUPABASE_ANON_KEY}"
        },
        "domains": [
          "{CLIENT_SUBDOMAIN}.seudominio.com"
        ],
        "ssl": {
          "enabled": true,
          "force_https": true,
          "auto_certificate": true
        },
        "resources": {
          "memory": "512Mi",
          "cpu": "0.5",
          "storage": "1Gi"
        },
        "scaling": {
          "min_replicas": 1,
          "max_replicas": 2,
          "target_cpu": 80
        }
      }
    ]
  },
  "clients": [
    {
      "number": "001",
      "name": "Cliente 001",
      "subdomain": "cliente001",
      "supabase_url": "https://SEU-PROJETO-001.supabase.co",
